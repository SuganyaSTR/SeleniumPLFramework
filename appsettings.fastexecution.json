{
  "WebDriver": {
    "Browser": "chrome",
    "Headless": true,
    "WindowSize": "1920x1080",
    "ImplicitWaitSeconds": 5,
    "PageLoadTimeoutSeconds": 15,
    "ScriptTimeoutSeconds": 10
  },
  "TestSettings": {
    "ScreenshotOnFailure": false,
    "VideoRecording": false,
    "DetailedLogging": false,
    "FastExecution": true
  },
  "Timeouts": {
    "ElementWaitTimeout": 3,
    "PageLoadTimeout": 15,
    "ScriptTimeout": 10
  },
  "Performance": {
    "DisableImages": true,
    "DisableCSS": false,
    "DisableJavaScript": false,
    "NetworkThrottling": false
  }
}
